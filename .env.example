# Copy this file to .env and fill in your secrets
# Example environment variables required by the backend

# MongoDB connection string (or use MONGO)
MONGO_URI=mongodb://localhost:27017/GigFlow

# JWT secret for signing tokens
JWT_KEY=your_jwt_secret_here

# Optional: frontend origin used by CORS (default in server.js)
# FRONTEND_URL=http://localhost:5173

# Note: Do NOT commit secrets to source control. Add .env to .gitignore if not already excluded.
